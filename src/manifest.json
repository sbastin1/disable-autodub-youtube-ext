{
  "{{chrome}}.manifest_version": 3,
  "{{firefox}}.manifest_version": 2,
  "name": "Disable AutoDubbing on Youtube",
  "description": "This Extensions disables the automatic ai dubbing for youtube videos",
  "version": "1.0",
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "96": "icon/96.png",
    "128": "icon/128.png"
  },
  "{{chrome}}.action": {
    "default_popup": "src/popup.html"
  },
  "{{firefox}}.browser_action": {
    "default_popup": "src/popup.html",
    "default_icon": "icon/32.png"
  },
  "content_scripts": [
    {
      "js": [
        "src/contentScripts/index.ts"
      ],
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ]
    }
  ]
}
